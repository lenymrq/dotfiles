general {
        colors = true
        interval = 5
}

order += backlight
order += volume_status
order += bluetooth
order += networkmanager
order += sysdata
order += battery_level
order += clock

backlight {
        button_up = 0
        button_down = 0
}

battery_level {
        format = "{icon}: {percent}%"
}

clock {
        format = "{Local}"
        format_time = "%F %H:%M:%S"
}
